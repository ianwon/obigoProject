   "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="callSettersOnNulls" value="true" />
	</settings>
	<typeAliases>

		<typeAlias type="com.obigo.obigoproject.vo.UsersVO" alias="Users" />
		<typeAlias type="com.obigo.obigoproject.vo.UserRequestVO"
			alias="UserRequest" />
		<typeAlias type="com.obigo.obigoproject.vo.UserVehicleVO"
			alias="UserVehicle" />
		<typeAlias type="com.obigo.obigoproject.vo.VehicleVO" alias="Vehicle" />
