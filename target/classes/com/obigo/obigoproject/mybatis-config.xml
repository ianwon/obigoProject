		<typeAlias type="com.obigo.obigoproject.vo.BundleVersionVO"
			alias="BundleVersion" />
		<typeAlias type="com.obigo.obigoproject.vo.ResourceVO"
			alias="Resource" />
		<typeAlias type="com.obigo.obigoproject.vo.PushMessageVO"
			alias="PushMessage" />
		<typeAlias type="com.obigo.obigoproject.vo.MessageCategoryVO"
			alias="MessageCategory" />
		<typeAlias type="com.obigo.obigoproject.vo.LogVO" alias="Log" />
		<typeAlias type="com.obigo.obigoproject.vo.ApiVO" alias="Api" />
		<typeAlias type="com.obigo.obigoproject.vo.UserMessageVO" alias="UserMessage" />
		<typeAlias type="com.obigo.obigoproject.vo.AndroidUserVehicleVO" alias="AndroidUserVehicle"/>
		<typeAlias type="com.obigo.obigoproject.vo.RegistrationidVO" alias="Registrationid"/>

	</typeAliases>

	<mappers>
		<mapper resource="com/obigo/obigoproject/UsersMapper.xml" />
		<mapper resource="com/obigo/obigoproject/UserRequestMapper.xml" />
		<mapper resource="com/obigo/obigoproject/UserVehicleMapper.xml" />
		<mapper resource="com/obigo/obigoproject/VehicleMapper.xml" />
		<mapper resource="com/obigo/obigoproject/BundleMapper.xml" />
		<mapper resource="com/obigo/obigoproject/BundleVersionMapper.xml" />
		<mapper resource="com/obigo/obigoproject/ResourceMapper.xml" />
		<mapper resource="com/obigo/obigoproject/PushMessageMapper.xml" />
		<mapper resource="com/obigo/obigoproject/MessageCategoryMapper.xml" />
		<mapper resource="com/obigo/obigoproject/LogMapper.xml" />
		<mapper resource="com/obigo/obigoproject/ApiMapper.xml" />
		<mapper resource="com/obigo/obigoproject/UserMessageMapper.xml" />
		<mapper resource="com/obigo/obigoproject/AndroidUserVehicleMapper.xml"/>
		<mapper resource="com/obigo/obigoproject/RegistrationidMapper.xml"/>
	</mappers>



</configuration>